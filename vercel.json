{
  "buildCommand": "cd apps/main && npm run build",
  "outputDirectory": "apps/main/.next",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "functions": {
    "apps/main/src/pages/**/*.tsx": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/rent/$1",
      "has": [
        {
          "type": "host",
          "value": "rent.mornhub.net"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/job/$1",
      "has": [
        {
          "type": "host",
          "value": "job.mornhub.net"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/social/$1",
      "has": [
        {
          "type": "host",
          "value": "social.mornhub.net"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/deepfake/$1",
      "has": [
        {
          "type": "host",
          "value": "deepfake.mornhub.net"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/accelerator/$1",
      "has": [
        {
          "type": "host",
          "value": "accelerator.mornhub.net"
        }
      ]
    }
  ]
} 