{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" },
    {
      "src": "apps/1_rent/next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "src": "/rent(/.*)?", "dest": "/apps/1_rent$1" },
    { "src": "/(.*)", "dest": "/apps/1_rent/$1", "host": "rent.mornhub.net" }
  ]
} 